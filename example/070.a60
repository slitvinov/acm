procedure AITKEN(x, f, n, X, F); real array x, f;
          integer n;  real X, F;
code;


begin
  integer n;
  ininteger(0, n);
  begin
      real X;
      real F;
      integer i;
      real array x[0:n], f[0:n];
      for i := 0 step 1 until n do
      begin
          inreal(0, x[i]);
          inreal(0, f[i])
      end;
      inreal(0, X);
      AITKEN(x, f, n, X, F);
      outreal(1, F);
      outstring(1, "\n")
  end
end
