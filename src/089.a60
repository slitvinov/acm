real procedure FRESNELSIN(u) Result : (frsin);
real u;
real frsin;
begin
real Pi2, x, frsqr, term, frsini;
integer N;
      Pi2 := 1.5707963; x := Pi2 * (u^2); frsin := x/3;
      frsqr := x^2; N := 3; term := (-x * frsqr)/6;
      frsini := frsin + term/7;
Loop: if frsin = frsini then go to exit;
      term := - term * frsqr/((2*N-1) * (2*N-2));
      frsini := frsin + term/(4*N - 1); N := N + 1;
      go to Loop;
exit: frsin := frsini * u
end FRESNELSIN;
